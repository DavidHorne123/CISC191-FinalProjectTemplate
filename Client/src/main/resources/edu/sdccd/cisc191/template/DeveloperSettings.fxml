<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.sdccd.cisc191.template.PlayerSettings.DeveloperSettingsController">
   <children>
      <Text layoutX="14.0" layoutY="79.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Settings">
         <font>
            <Font size="60.0" />
         </font>
      </Text>
      <GridPane fx:id="gridPane" layoutX="65.0" layoutY="136.0" prefHeight="673.0" prefWidth="418.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="842.0" minWidth="10.0" prefWidth="418.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="475.0" minWidth="0.0" prefWidth="0.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" onAction="#setPlayerMaxMoney" prefHeight="293.0" prefWidth="418.0" text="Max Money">
               <font>
                  <Font size="35.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#resetPlayerInventory" prefHeight="293.0" prefWidth="418.0" text="Delete Inventory" GridPane.rowIndex="2">
               <font>
                  <Font size="35.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#resetPlayerMoney" prefHeight="293.0" prefWidth="418.0" text="Remove Money" GridPane.rowIndex="1">
               <font>
                  <Font size="35.0" />
               </font>
            </Button>
         </children>
      </GridPane>
      <Button layoutX="14.0" layoutY="884.0" mnemonicParsing="false" onAction="#switchToSettingsScene" prefHeight="100.0" prefWidth="200.0" text="Back">
         <font>
            <Font size="48.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
